if game.Workspace:FindFirstChild("nil") then
game.Workspace:FindFirstChild("nil"):Destroy()
print("Bypassed")
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()
local Window = Library.CreateLib("Smilley", "RJTheme3")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local i = 0
local RunService = game:GetService("RunService")

local Tab = Window:NewTab("Main")

-- Подсекция
local Section = Tab:NewSection("Functions")

Section:NewButton("Attack Spam 2", "Spamming Your and other player attacks", function()
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local tool = Instance.new("Tool")
tool.Name = "Our Spammer"
tool.RequiresHandle = true
local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(1, 1, 3)
handle.CanCollide = false
handle.Anchored = false
handle.Transparency = 1
handle.Parent = tool
local function fireRemotes(obj, pos)
    for _, child in ipairs(obj:GetDescendants()) do
        if child:IsA("RemoteEvent") then
            pcall(function()
                child:FireServer(pos.X, pos.Y, pos.Z)
            end)
        end
    end
end
tool.Activated:Connect(function()
    local attackPos = mouse.Hit.Position
    local sources = { 
        player:FindFirstChild("Backpack"), 
        player.Character,
        player:FindFirstChild("PlayerGui")
    }
    for _, source in ipairs(sources) do
        if source then fireRemotes(source, attackPos) end
        task.wait()
    end
    for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
        if otherPlayer == player then continue end
        
        local otherBackpack = otherPlayer:FindFirstChild("Backpack")
        if otherBackpack then fireRemotes(otherBackpack, attackPos) end
        
        local otherCharacter = otherPlayer.Character
        if otherCharacter then fireRemotes(otherCharacter, attackPos) end
        
        task.wait()
    end
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Tool") then fireRemotes(obj, attackPos) end
    end
end)
tool.Parent = player.Backpack
end)

Section:NewButton("Attack Spam", "spaming only your attacks", function()
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

local tool = Instance.new("Tool")
tool.Name = "Self Spammer"
tool.RequiresHandle = true

local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(1, 1, 3)
handle.CanCollide = false
handle.Anchored = false
handle.Transparency = 1
handle.Parent = tool

tool.Activated:Connect(function()
    task.spawn(function()
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid", 5)
        local rootPart = character:WaitForChild("HumanoidRootPart", 5)
        local attackPos = mouse.Hit.Position  -- Vector3
        local sources = { player:FindFirstChild("Backpack"), character }
        for _, source in ipairs(sources) do
            if not source then continue end
            for _, obj in ipairs(source:GetDescendants()) do
                if obj:IsA("RemoteEvent") then
                    local success, err = pcall(function()
                        obj:FireServer(attackPos.X, attackPos.Y, attackPos.Z)
                    end)
                end
            end
            wait(0.00001)
        end
    end)
end)

tool.Parent = player:FindFirstChild("Backpack") or player.Backpack
end)

Section:NewSlider("Speed Hack", "Adding Speed To Your Humanoid", 200, 0, function(s) -- 200 (Макс. значение) | 0 (Мин. значение)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Section:NewButton("Soul AutoFarm", "Auto collect souls", function()
RunService.RenderStepped:Connect(function(deltaTime)
for i , obj in pairs(game.Workspace.SoulsStorage:GetChildren()) do 
		if obj:IsA("Model") then
			if obj:GetChildren()[1] then
				obj:GetChildren()[1].CFrame = player.Character.HumanoidRootPart.CFrame
			end
		end
	end
end)
end)

Section:NewButton("ShortCut (No CD)", "Click Teleport Tool", function()
mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "ShortCut"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

Section:NewToggle("GodMode(BETA)", "God Mode (Noclip)", function(state)
    local toggleEnabled = state
    local function applyGodMode(char)
        if not char then return end
        
        for _, part in pairs(char:GetChildren()) do
            if part:IsA("BasePart") or part.Name == "Hitbox" then
                part.CanTouch = not toggleEnabled
            end
        end
    end
    applyGodMode(player.Character)

    local connection
    if toggleEnabled then
        connection = player.CharacterAdded:Connect(function(newChar)
            wait(0.1)
            if toggleEnabled then
                applyGodMode(newChar)
            end
        end)
    else
        if connection then
            connection:Disconnect()
        end
    end
end)


Section:NewButton("Esp", "Highlight Players", function()
while wait(0.02) do
for i , obj in pairs(game.Players:GetChildren()) do 
local s = Instance.new("Highlight")
local p = s:Clone()
p.Parent = obj.Character
end
end
end)



Section:NewToggle("All Attacks", "Gives All Attacks", function(state)
    if state then
        for i , obj in pairs(game.ReplicatedStorage:GetDescendants()) do
            if obj:IsA("Tool") then
            obj.Parent = player.Backpack
            end
        end
    else
        for i , obj in pairs(player.Backpack:GetChildren()) do
            obj.Parent = game.ReplicatedStorage
        end
    end
end)

local Tab = Window:NewTab("Locations")

