player.Character:PivotTo(CFrame.new(2465.5498046875, 143.39999389648438, 7824.29150390625))
end)

Section:NewButton("Waterfall", "Auto collect souls", function()
player.Character:PivotTo(CFrame.new(383.003326, 277.724091, 3337.79224, -0.999045789, -1.04182561e-07, -0.0436746664, -1.06346825e-07, 1, 4.72307384e-08, 0.0436746664, 5.18303338e-08, -0.999045789))
end)

Section:NewButton("Snowdin", "Auto collect souls", function()
player.Character:PivotTo(CFrame.new(4723.77197, -3952.52832, 5360.59863, 0.999936044, 1.17829984e-07, 0.0113080163, -1.17828982e-07, 1, -7.54959373e-10, -0.0113080163, -5.77500936e-10, 0.999936044))
end)

Section:NewButton("Lobby", "Auto collect souls", function()
player.Character:PivotTo(CFrame.new(-58.5165482, -3734.00024, 0.550065577, 0.0143970484, -2.31800072e-08, 0.999896348, 8.28385183e-08, 1, 2.19896563e-08, -0.999896348, 8.25133455e-08, 0.0143970484))
end)

